{
  "name": "rezonans/rezonans",
  "description": "Build your modern Application with Wordpress and modern front, on Docker with CI",
  "type": "project",
  "keywords": ["wp", "wordpress", "rezonans", "docker", "CI"],
  "authors": [
    {
      "name": "Alexandr Shevchenko",
      "email": "ls.shov@gmail.com"
    }
  ],
  "license": "BSD-2-Clause",
  "repositories":[
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=7.1",
    "johnpbloch/wordpress": "^4.9",
    "wpackagist-plugin/advanced-custom-fields": "^4.0",
    "rezonans/rezonans-core": "dev-11-make-installation-and-basic-configuration-of-the-wordpress-the-automatic-after-creating-the-project"
  },
  "extra": {
    "installer-paths": {
      "wordpress/wp-content/plugins/{$name}": ["type:wordpress-plugin"]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/app",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "bin/post_install"
    ]
  }
}